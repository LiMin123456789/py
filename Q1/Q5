x=int(input('请输入第一个数：'))
y=int(input('请输入第二个数：'))
z=int(input('请输入第三个数：'))
t=0
if x<y:
    if x<z:
        print(x)
        if y<z:
            print(y)
            print(z)
        else:
            print(z)
            print(y)
    else:
        print(z,'\n',x,'\n',y)
else:
    if y<z:
        print(y)
        if x<z:
            print(x,'\n',z)
        else:
            print(z,'\n',x)
    else:
        print(z,'\n',y,'\n',x)


a=[x,y,z]
a.sort()
print (a[0],a[1],a[2])
